digraph foo {
rankdir=LR ;
ranksep =".25" ;

/* default shape for nodes */
node [shape=circle, style=solid, fontsize=12]
edge [fontsize=12] ;

s0 -> s1 [label="c:c"] ;
s0 [style=bold] ; 
s1 -> s2 [label="a:a"] ;
s2 -> s3 [label="t:t"] ;
s3 -> s4 [label="[Noun]:[Noun]"] ;
s4 -> s5 [label="[Sg]:[Sg]"] ;
s5 [shape=doublecircle] ;

}
